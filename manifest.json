{
    "manifest_version": 3,
    "name": "Nomoresurf",
    "version": "1.0",
    "description": "Web site blocker",
    "options_page": "options.html",
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "storage",
        "tabs"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "/images/nomoresurf_16.png",
            "32": "/images/nomoresurf_32.png",
            "48": "/images/nomoresurf_48.png",
            "128": "/images/nomoresurf_128.png"
        }
    },
    "icons": {
        "16": "/images/nomoresurf_16.png",
        "32": "/images/nomoresurf_32.png",
        "48": "/images/nomoresurf_48.png",
        "128": "/images/nomoresurf_128.png"
    }
}